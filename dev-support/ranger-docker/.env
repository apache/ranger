BUILD_HOST_SRC=true
SKIPTESTS=true
GIT_URL=https://github.com/apache/ranger.git
BRANCH=ranger-2.8
PROFILE=
BUILD_OPTS=

# DockerHub Ranger Base Image
RANGER_BASE_IMAGE=apache/ranger-base
# Java version used to run Ranger and dependent services is present as suffix: -8, valid values for suffix: -8, -11, -17
RANGER_BASE_VERSION=20260123-2-8
# Java version used to build Apache Ranger is present as suffix: -8, valid values for suffix: -8, -11, -17
RANGER_BASE_BUILD_VERSION=20260123-2-8

# third party image versions
MARIADB_VERSION=10.7.3
POSTGRES_VERSION=13.16
ORACLE_VERSION=23.6
SQLSERVER_VERSION=2022-latest
ENABLE_DB_MOUNT=true
ZK_VERSION=3.9.2
SOLR_VERSION=8.11.3

# service versions
HADOOP_VERSION=3.4.2
HBASE_VERSION=2.6.0
HIVE_VERSION=3.1.3
HIVE_HADOOP_VERSION=3.4.2
KAFKA_VERSION=3.9.1
KNOX_VERSION=2.0.0
TRINO_VERSION=latest

# Kerberos
KERBEROS_ENABLED=true
KERBEROS_REALM=EXAMPLE.COM
KERBEROS_KDC_HOST=ranger-kdc.example.com
KERBEROS_MASTER_PASSWORD=rangerR0cks!
KERBEROS_ADMIN_PRINCIPAL=admin/admin
KERBEROS_ADMIN_PASSWORD=rangerR0cks!

OZONE_VERSION=1.4.0
OZONE_RUNNER_VERSION=20230615-1
OZONE_RUNNER_IMAGE=apache/ozone-runner

# versions of ranger services
RANGER_VERSION=2.8.0
KMS_VERSION=2.8.0
USERSYNC_VERSION=2.8.0
TAGSYNC_VERSION=2.8.0

# plugin versions
HDFS_PLUGIN_VERSION=2.8.0
YARN_PLUGIN_VERSION=2.8.0
HIVE_PLUGIN_VERSION=2.8.0
HBASE_PLUGIN_VERSION=2.8.0
KAFKA_PLUGIN_VERSION=2.8.0
KNOX_PLUGIN_VERSION=2.8.0
OZONE_PLUGIN_VERSION=2.8.0
SOLR_PLUGIN_VERSION=2.8.0

# To enable debug logs
DEBUG_ADMIN=false
DEBUG_USERSYNC=false
DEBUG_TAGSYNC=false
